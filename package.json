{
  "name": "react-boilerplate-dev-proxy",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others 'npm run start:server' 'cd client; npm run start'",
    "install:all": "npm run install; cd client; npm run install; cd ..",
    "start:server": "nodemon ./bin/www --ignore 'client/*'",
    "post-yarn": "sh ./tools/yarn-scripts/yarn.sh"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "express": "^4.16.4",
    "http-proxy": "^1.17.0",
    "morgan": "^1.9.0",
    "serve-favicon": "^2.5.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.17.3"
  }
}
