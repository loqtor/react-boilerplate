{
  "name": "react-boilerplate-web",
  "version": "0.0.2",
  "private": true,
  "dependencies": {
    "axe-core": "^3.1.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^5.0.0",
    "core-js": "^2.6.3",
    "downshift": "^3.1.5",
    "history": "^4.7.2",
    "humps": "^2.0.1",
    "lodash.debounce": "^4.0.8",
    "node-sass": "^4.10.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.6.0",
    "react-app-polyfill": "^0.2.0",
    "react-axe": "^3.0.2",
    "react-dom": "^16.6.0",
    "react-ga": "^2.5.3",
    "react-id-swiper": "^1.6.8",
    "react-intl": "^2.7.2",
    "react-onclickoutside": "^6.7.1",
    "react-pose": "^4.0.1",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "redux": "^4.0.1",
    "redux-action-creator": "^3.0.0",
    "redux-responsive": "^4.3.8",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0",
    "scroll-into-view": "^1.9.3",
    "svg-sprite-generator": "^0.0.7",
    "uuid": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint && CI=true npm run test"
    }
  },
  "scripts": {
    "format": "prettier-eslint --write \"./src/**/*.js\"",
    "lint": "eslint --config ./.eslintrc ./src --ext .js,.jsx",
    "build-storybook": "build-storybook -s public",
    "build": "npm run run react-scripts build",
    "eject": "react-scripts eject",
    "post-yarn": "sh ../tools/yarn-scripts/yarn.sh",
    "start": "PORT=3002 react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "generate-svgs": "echo 'CHECK THAT YOUR SVG HAS A VIEW BOX, otherwise it will not work'; rm ./src/all.svg; svg-sprite-generate -d ./public/assets/svg-icons -o ./src/all.svg; node ./tools/svg-generator",
    "generate-icons": "node ./tools/icomoon-generator",
    "preversion": "CI=true npm run test",
    "version": "",
    "postversion": "git push && git push --tags"
  },
  "proxy": "http://localhost:3001",
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addon-storyshots": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.27.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "prettier": "^1.15.3",
    "prettier-eslint-cli": "^4.7.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
